<Application
    x:Class="RemoteCamera.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:RemoteCamera.Converters"
    RequestedTheme="Light">

    <Application.Resources>
        <converters:LogicalNegationConverter x:Key="LogicalNegationConverter" />
        <converters:BrightnessStringConverter x:Key="BrightnessStringConverter" />
    </Application.Resources>
</Application>    
